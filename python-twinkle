x = 'Twinkle, twinkle, little star,'
y = ' How I wonder what you are!'
z = 'Up above the world so high,'
zx = 'Like a diamond in the sky,'
print(x,'\n','\t',y,'\n','\t','\t',z,'\n',zx,'\n',x,'\n',y)
